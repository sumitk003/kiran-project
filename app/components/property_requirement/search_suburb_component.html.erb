<%= form_with(url: property_requirements_search_suburbs_path, data: { controller: 'property-requirements--search-suburb' }) do |form| %>
  <label for="Search suburb" class="block text-sm font-medium leading-6 text-gray-900">Suburb search</label>
  <div class="relative mt-2 rounded-md shadow-sm">
    <div class="pointer-events-none absolute inset-y-0 left-0 flex items-center pl-3">
      <svg xmlns="http://www.w3.org/2000/svg" class="h-5 w-5 text-gray-400" fill="none" viewBox="0 0 24 24" stroke-width="1.5" stroke="currentColor">
        <path stroke-linecap="round" stroke-linejoin="round" d="M21 21l-5.197-5.197m0 0A7.5 7.5 0 105.196 5.196a7.5 7.5 0 0010.607 10.607z" />
      </svg>
    </div>
    <%= form.text_field :suburb_name, data: { action: "input->property-requirements--search-suburb#search" }, class: 'block w-full rounded-md border-0 py-1.5 pl-10 text-gray-900 ring-1 ring-inset ring-gray-300 placeholder:text-gray-400 focus:ring-2 focus:ring-inset focus:ring-indigo-600 sm:text-sm sm:leading-6', autocomplete: :off %>
  </div>

  <div id="search_suburb_results">
    <ul>
    </ul>
  </div>
<% end %>



